create table TestStudents(
sno CHAR(8),
firstname VARCHAR(12),
lastname VARCHAR(15),
phoneno CHAR(10)
);
create table TestUnits(
sno CHAR(8),
unitindex CHAR(8),
dept CHAR(10),
unitname VARCHAR(40)
);
create table TestEnrolmentss(
unit CHAR(8),
student CHAR(8),
year CHAR(4),
mark INT(3),
grade CHAR(1)
);
